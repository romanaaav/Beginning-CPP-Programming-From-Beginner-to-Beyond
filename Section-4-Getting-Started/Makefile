.PHONY: clean All

All:
	@echo "----------Building project:[ 5-CompilerErrors - Debug ]----------"
	@cd "CompilerErrors" && "$(MAKE)" -f  "5-CompilerErrors.mk"
clean:
	@echo "----------Cleaning project:[ 5-CompilerErrors - Debug ]----------"
	@cd "CompilerErrors" && "$(MAKE)" -f  "5-CompilerErrors.mk" clean
